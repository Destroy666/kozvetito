@model IEnumerable<kozvetito.Areas.allas.Models.SzgepIsmFelh>

@if (Model.Count() != 0)
{
    <h3>
        Felhasználói Ismeretek
    </h3>

    <table class="nyelvTable">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SzotarSzgepIsmFelh.Hu)
                    -
                    @Html.DisplayFor(modelItem => item.SzotarTudasszint.Hu)
                </td>
                <td>
                    @Html.ActionLink("Törlés", "FelhIsmTorles", new {id = item.Id})
                </td>
            </tr>
        }

    </table>
}