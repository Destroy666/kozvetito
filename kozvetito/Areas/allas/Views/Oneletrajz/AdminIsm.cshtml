@using System.Activities.Statements
@model IEnumerable<kozvetito.Areas.allas.Models.SzgepIsmAdmin>

@if (Model.Count() != 0)
{
    <h3>
        Adminisztrációs ismeretek
    </h3>
    <table class="nyelvTable">

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SzotarSzgepIsmAdmin.Hu)
                    -
                    @Html.DisplayFor(modelItem => item.SzotarTudasszint.Hu)
                </td>
                <td>
                    @Html.ActionLink("Törlés", "AdminTorles", new {id = item.Id})
                </td>
            </tr>
        }

    </table>
}